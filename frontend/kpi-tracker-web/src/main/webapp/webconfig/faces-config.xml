<?xml version="1.0" encoding="utf-8"?>
<faces-config version="2.0"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

	<name>primefaces_california</name>

	<component>
		<component-type>org.primefaces.component.CaliforniaMenu</component-type>
		<component-class>org.primefaces.california.component.CaliforniaMenu</component-class>
	</component>

	<render-kit>
		<renderer>
			<component-family>org.primefaces.component</component-family>
			<renderer-type>org.primefaces.component.CaliforniaMenuRenderer</renderer-type>
			<renderer-class>org.primefaces.california.component.CaliforniaMenuRenderer</renderer-class>
		</renderer>
	</render-kit>

	<navigation-rule>
	</navigation-rule>
	<managed-bean>
		<managed-bean-name>passwordResetController</managed-bean-name>

		<managed-bean-class>org.pahappa.systems.kpiTracker.security.PasswordResetController</managed-bean-class>

		<managed-bean-scope>view</managed-bean-scope>

		<managed-property>
			<property-name>passwordResetService</property-name>
			<value>#{passwordResetService}</value>
		</managed-property>
	</managed-bean>

	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
		<action-listener>org.primefaces.application.DialogActionListener</action-listener>
		<navigation-handler>org.primefaces.application.DialogNavigationHandler</navigation-handler>
		<view-handler>org.primefaces.application.DialogViewHandler</view-handler>
	</application>

</faces-config>
